<h1>Foods</h1>


<table border="1">
    <tr>
        <th>Name</th>
        <th>Category</th>
        <th>Fat</th>
        <th>Protein</th>
        <th>Carbohydrates</th>
    </tr>
<% @foods.each do |food| %>
    <tr>
        <td><%=food.name%></td>
        <td><%=food.category%></td>
        <td><%=food.fat%></td>
        <td><%=food.protein%></td>
        <td><%=food.carbohydrates%></td>
    </tr>


<% end %>
</table>